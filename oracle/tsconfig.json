{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "lib": ["es2022"],
    "target": "es2022",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": ".",
    "outDir": "build"
  },
  "include": [
    "node_modules/**/*.ts",
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
